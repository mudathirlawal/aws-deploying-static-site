{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/index.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Mocha - Current File",
      "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
        "args": [
            "--timeout",
            "999999",
            "--colors",
            "${file}"
        ],
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "env": {
          "PS_S3_BUCKET_NAME": "ps-ccp-a4321",
          "PS_AWS_ACCOUNT_ID": "234640428542",
          "PS_S3_WEBHOST_URL": "http://ps-ccp-a4321.s3-website-us-east-1.amazonaws.com",
          "PS_S3_CF_BUCKET_NAME": "ps-ccp-a4322",
          "PS_CF_DOMAIN_NAME": "d3gcblhpajhhh0.cloudfront.net"
        }
    }
  ]
}