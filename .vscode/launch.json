{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/index.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Mocha - Current File",
      "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
        "args": [
            "--timeout",
            "999999",
            "--colors",
            "${file}"
        ],
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "env": {
          "PS_S3_BUCKET_NAME": "test-bucket-a1020304506",
          "PS_AWS_ACCOUNT_ID": "111122223333",
          "PS_S3_WEBHOST_URL": "http://test-bucket-a1020304506.s3-website-us-east-1.amazonaws.com",
          "PS_S3_CF_BUCKET_NAME": "test-bucket-a1020304507",
          "PS_CF_DOMAIN_NAME": "d22re0vv5oup7i.cloudfront.net"
        }
    }
  ]
}