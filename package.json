{
  "name": "aws-deploying-static-site",
  "version": "1.0.0",
  "description": "Project for deploying an AWS static site on S3 and CloudFront",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha",
    "clean": "npx rimraf assets/group3/* assets/group2/*",
    "sync:services-quiz": "aws s3 sync s3://ps-ccp-project/ps-services-quiz/ assets/group2/ --delete",
    "sync:community": "aws s3 sync s3://ps-ccp-project/ps-community/ assets/group3/ --delete",
    "sync:all": "npm run sync:services-quiz && npm run sync:community",
    "sync": "npm run clean && npm run sync:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pluralsight-projects/aws-deploying-static-site.git"
  },
  "author": "David Tucker",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pluralsight-projects/aws-deploying-static-site/issues"
  },
  "homepage": "https://github.com/pluralsight-projects/aws-deploying-static-site#readme",
  "dependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "chai-string": "^1.5.0",
    "mocha": "^7.1.1",
    "rimraf": "^3.0.2"
  }
}
